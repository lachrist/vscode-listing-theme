{
	"name": "Listing",
	"colors": {
		"editor.background": "#FFFFFF",
		"editor.foreground": "#000000",
    "list.activeSelectionIconForeground": "#FFFFFF"
	},
	"tokenColors": [
		{
			"name": "Variable",
			"scope": ["variable", "meta.expression.scheme", "entity"],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "Comment",
			"scope": ["comment", "markup"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#888888"
			}
		},
		{
			"name": "Punctuation",
			"scope": ["punctuation", "keyword.operator"],
			"settings": {
				"foreground": "#888888"
			}
		},
		{
			"name": "String",
			"scope": ["string", "constant.character"],
			"settings": {
				"foreground": "#6A9955"
			}
		},
		{
			"name": "Special",
			"scope": ["variable.language", "storage.type"],
			"settings": {
				"foreground": "#D45D00"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword",
				"storage.modifier",
				"storage.type.js",
				"storage.type.property.js",
				"storage.type.function.js",
				"storage.type.function.arrow.js",
				"storage.type.class.js",
				"storage.type.python",
				"storage.type.function.python",
				"storage.type.class.python",
			],
			"settings": {
				"foreground": "#7A3E9D"
			}
		},
		{
			"name": "Constant",
			"scope": "constant",
			"settings": {
				"foreground": "#4682B4"
			}
		},
		{
			"name": "Meta",
			"scope": ["meta", "invalid"],
			"settings": {
				"foreground": "#8B0000"
			}
		}
	]
}
